{
  "homepage": "https://jorgearancibia1.github.io/cv-vite",
  "name": "cv-vite",
  "private": true,
  "version": "1.0.2",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:optimized": "tsc && vite build --base=/cv-vite/",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "toc": "i18next-rosources-for-ts toc -i ./src/locales/en -o ./src/@types/resources.ts",
    "interface": "i18next-rosources-for-ts interface -i ./src/locales/en -o ./src/@types/resources.d.ts"
  },
  "dependencies": {
    "i18next": "23.7.16",
    "i18next-http-backend": "2.4.2",
    "i18next-resources-for-ts": "1.5.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-i18next": "14.0.0",
    "react-icons": "4.12.0"
  },
  "devDependencies": {
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@typescript-eslint/eslint-plugin": "6.19.0",
    "@typescript-eslint/parser": "6.19.0",
    "@vitejs/plugin-react-swc": "3.0.0",
    "eslint": "8.56.0",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-refresh": "0.3.4",
    "gh-pages": "6.1.1",
    "typescript": "5.0.2",
    "vite": "4.5.1"
  }
}